rankcor=function (x,y) {
xrk=rank (x)
yrk=rank (y)
rr=vector (length=length(x))
for (i in 1:length(x)) {
ifelse(xrk[i]==yrk[i],rr[i]=1,rr[i]=0)
}
return(mean(rr))
}
